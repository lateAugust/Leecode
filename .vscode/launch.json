  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run dev",
      "name": "start DEV",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "command": "npm run build:electron",
      "name": "build electron",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}
